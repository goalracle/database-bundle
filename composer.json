{
  "name": "torakel/database-bundle",
  "type": "football-database-bundle",
  "description": "The database bundle for the Torakel project",
  "keywords": ["symfony2", "doctrine", "database", "football", "sport"],
  "license": "MIT",
  "homepage": "https://github.com/torakel/database-bundle",
  "authors": [
    {
      "name": "Nils Abegg",
      "email": "nils.abegg@gmail.com",
      "homepage": "http://www.torakel.com/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/config": "4.0.*",
    "symfony/dependency-injection": "4.0.*",
    "symfony/http-kernel": "4.0.*",
    "doctrine/doctrine-bundle": "^1.8"
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.0",
    "sami/sami": "^4.0",
    "symfony/phpunit-bridge": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "Torakel\\DatabaseBundle\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Torakel\\DatabaseBundle\\Tests\\": "./tests/" }
  }
}